@model LocalListViewModel

<div>
    <h2>@Model.ServicoAtual</h2><br />
</div>

<div class="container">
    <div class="row">
            @foreach (var local in Model.Locals)
            {
                <div class="col-sm-4 col-md-4 col-lg-4">
                <h4>@local.Cidade</h4>
                <h4>@local.Bairro</h4><br />
                </div>
            }
    </div>
</div>