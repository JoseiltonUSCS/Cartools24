@model IEnumerable<Cartools.Models.Oficina>

<div>
    <h2>Todas as Oficinas</h2><br />
</div>

<div class="container">
    <div class="row">
        @foreach (var oficina in Model)
        {
            <div class="col-sm-4 col-md-4 col-lg-4">
                <h4>@oficina.OficinaNome</h4>
                <h4>@oficina.OficinaTelefone</h4> <br /><br /><br /><br />
            </div>
        }
    </div>
</div>